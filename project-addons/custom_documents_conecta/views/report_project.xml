<?xml version="1.0"?>
<odoo>



<template id="report_project_hours_document">
    <t t-call="custom_documents_conecta.custom_external_layout">
        <t t-set="doc" t-value="o.with_context(lang=o.partner_id.lang)" /> 
        <h4>
            HORAS
        </h4>
        <span t-esc="eee"></span>
    </t>
</template>




<template id="report_project_hours">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-set="lang" t-value="o.partner_id and o.partner_id.lang"/>
            <t t-call="custom_documents_conecta.report_project_hours_document" t-lang="lang"/>
        </t>
    </t>
</template>

</odoo>
